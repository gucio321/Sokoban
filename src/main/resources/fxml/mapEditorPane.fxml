<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="820.0" prefWidth="1457.0"
            style="-fx-background-image: url('/img/gui/backgroundEditor.png')"
            xmlns="http://javafx.com/javafx/8.0.202-ea"
            fx:controller="pl.crystalek.sokoban.controller.MapEditorController">
    <children>
        <Button layoutX="8.0" layoutY="531.0" mnemonicParsing="false" onAction="#leave" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Opuść edytor" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
        <Button layoutX="8.0" layoutY="288.0" mnemonicParsing="false" onAction="#changeMapName" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Zmień nazwę mapy" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="13.0"/>
            </font>
        </Button>
        <Button layoutX="8.0" layoutY="240.0" mnemonicParsing="false" onAction="#saveMap" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Zapisz mapę" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
        <Button layoutX="8.0" layoutY="383.0" mnemonicParsing="false" onAction="#showMapList" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Lista zapisanych map" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="12.0"/>
            </font>
        </Button>
        <Button layoutX="8.0" layoutY="335.0" mnemonicParsing="false" onAction="#resetMap" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Zresetuj mapę" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
        <AnchorPane fx:id="editedAreaPane" layoutX="160.0" layoutY="20.0" prefHeight="780.0" prefWidth="1289.0">
            <children>
                <GridPane fx:id="mapBox" gridLinesVisible="true" prefHeight="780.0" prefWidth="1170.0">
                </GridPane>
                <VBox fx:id="blockBox" alignment="CENTER" layoutX="1194.0" layoutY="160.0" prefHeight="460.0"
                      prefWidth="71.0" spacing="20.0" style="-fx-background-color: #bbbbbb;"/>
            </children>
        </AnchorPane>
        <Button layoutX="8.0" layoutY="431.0" mnemonicParsing="false" onAction="#importMap" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Import mapy" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
        <Button layoutX="8.0" layoutY="480.0" mnemonicParsing="false" onAction="#exportMap" prefHeight="37.0"
                prefWidth="138.0" style="-fx-background-color: #bbbbbb;" text="Eksport mapy" textFill="#a10000">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
